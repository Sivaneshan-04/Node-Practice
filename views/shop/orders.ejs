<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if(orders.length <=0 ) { %>
                <h1>Nothing there!</h1>
            <% }  else { %>
                <div>
                    <ul>
                        <% orders.forEach(order=>{ %>
                            <li>
                                <p> <%= order._id %>-----------<span><a href="/orders/<%= order._id %>">Download your Invoice</a></span> </>
                                <ul>
                                    <% order.prods.forEach((item=>{ %>
                                        <li><%= item.productData.title %> (<%= item.quantity %>)</li>
                                    <% })); %>
                                </ul>
                            </li>
                        <% }); %>
                    </ul>
                </div>

            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>